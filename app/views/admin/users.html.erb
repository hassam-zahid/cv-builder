
<div class="container">
  <h2>User Details</h2>
  <p>Total Users - <%= @users.count %></p>            
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Email</th>
        <th>Country</th>
        <th>Date</th>
      </tr>
    </thead>
    <tbody>
    <%@users.reverse.each do |user|%>
      <tr>
        <td><%= user.email%></td>
        <td><%= user.city.country.name %></td>
        <td><%= user.created_at%></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
