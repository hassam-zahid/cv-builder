
<div class="row">
    <div class="col-sm-5">
      <%=  render 'resume/resume_title'%>
    </div>
    
    <div class="col-sm-7">
        <div id="notify_122" style="margin-left: 12%;">
        
        </div>
    </div>
</div>



<div class="progress" style="margin: 20px 0px;">
  <div class="progress-bar" role="progressbar" style="width: 15%;" aria-valuenow="15" aria-valuemin="0" aria-valuemax="100">15%</div>
</div>

<ul class="nav nav-tabs" role="tablist">
  <li class="nav-item">
    <a class="nav-link active" data-toggle="tab" href="#profile" role="tab"><%= t('resume.profile')%>
    <span><%= render 'resume/completion_ratio_tabs', data: @completion_ratio[:profile] %></span>
    </a>
  </li>
  <li class="nav-item">
    <a class="nav-link" data-toggle="tab" href="#experiences" role="tab"><%= t('resume.experiences')%>
      <span id="exp_cp_ratio"><%= render 'resume/completion_ratio_tabs', data: @completion_ratio[:employment_experience] %></span>
    </a>
  </li>
  <li class="nav-item">
    <a class="nav-link" data-toggle="tab" href="#skills" role="tab"><%= t('resume.skills')%></a>
  </li>
  <li class="nav-item">
    <a class="nav-link" data-toggle="tab" href="#education" role="tab"><%= t('resume.education')%></a>
  </li>
  
  <li class="nav-item">
    <a class="nav-link" data-toggle="tab" href="#templates" id="templates_view_dL" role="tab"><%= t('resume.templates')%></a>
  </li>
</ul>


<div class="tab-content">
  <div class="tab-pane active" id="profile" role="tabpanel">
  	<%= render 'resume/profile'%>
  </div>
  <div class="tab-pane" id="experiences" role="tabpanel">
    <%= render 'resume/employment_experience'%>
  </div>
  <div class="tab-pane" id="skills" role="tabpanel">
      <%= render 'resume/skills'%>
  </div>
  <div class="tab-pane" id="education" role="tabpanel">
    <%= t('resume.certificates')%><br>
    <%= t('resume.achievements')%>
  </div>
  <div class="tab-pane" id="templates" role="tabpanel">
      <%= render 'resume/templates'%>
  </div>
</div>



